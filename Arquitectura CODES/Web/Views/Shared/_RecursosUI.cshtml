<script type="text/javascript">

	@{
		System.Resources.ResourceManager MyResourceClass = new System.Resources.ResourceManager(typeof(ARQ.Recursos.Global));
		System.Resources.ResourceSet resourceSet = MyResourceClass.GetResourceSet(System.Globalization.CultureInfo.CurrentUICulture, true, true);

		List<KeyValuePair<string, string>> data = new List<KeyValuePair<string, string>>();
		foreach (System.Collections.DictionaryEntry resourceItem in resourceSet)
		{
			data.Add(new KeyValuePair<string, string>(resourceItem.Key.ToString(), resourceItem.Value.ToString()));
		}
		var resxString = Newtonsoft.Json.JsonConvert.SerializeObject(data).Replace("'", "\"");
	}

		var jsglobalsAux = JSON.parse('@Html.Raw(resxString)');

	var jsglobals = {};

	for (var i = 0; i < jsglobalsAux.length; i++) {
		jsglobals[jsglobalsAux[i].Key] = jsglobalsAux[i].Value
	}

	vueAppParams.data.jsglobals = jsglobals;

</script>